	"DataPicker Lowdefy": {
		"prefix": "datapicker",
		"body": [
			"- id: $1",
			"\ttype: DateSelector",
			"\tlayout: ",
			"\tstyle: ",
			"\t\tspan: $2",
			"\trequired: false",
			"\tproperties: ",
			"\t\ttitle: $3",
			"\t\tplaceholder: $4",
			"\t\tdisabledDates:",
			"\t\t\tdates: []",
			"\t\t\tranges:",
			"\t\t\tmax:",
			"\t\t\t\t_data:",
			"\t\tformat: DD/MM/YYYY"
		],
		"description": "Date selection block with maximum date preset for current day with day/month/year date format"
	},
	"TextInputWhitValidate Lowdefy": {
		"prefix": "textInputWhitValidate",
		"body": [
			"- id: $1",
			"\ttype: TextInput",
			"\tlayout: ",
			"\t\tspan: $2 ",
			"\trequired: false",
			"\tvalidate:",
			"\t\t- message: $3",
			"\t\t\tstatus: error",
			"\t\t\tpass:",
			"\t\t\t#regex that only allows numbers and letters",
			"\t\t\t\t_regex: '^[A-Za-záàâãéèêíïóôõöúçñÁÀÂÃÉÈÍÏÓÔÕÖÚÇÑ 0-9]+$' ",
			"\tproperties: ",
			"\t\ttitle: $4",
			"\t\tplaceholder: $5",
			"\t\tdisabled: false"
		],
		"description": "Text input with validation and regex"
	},
